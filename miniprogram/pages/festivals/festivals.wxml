<view class="search">
  <SearchInput></SearchInput>
</view>
<view class="festivals_swiper">
   <swiper class="" indicator-dots="false" indicator-color="rgba(0,0,0,.3)" autoplay="true" interval="5000" duration="1000" circular="{{true}}" >
     <swiper-item wx:for="{{festivals_swiper_images}}"
            wx:key="fileID">
     <navigator class="" target="" url="" hover-class="navigator-hover" open-type="navigate" ci>
       <image  src="{{item.tempFileURL}}" mode="wideFix"  /> 
     </navigator> 
     </swiper-item>
     
   </swiper>
</view>

<view class="year_start_father">
    <view class="year_start_son">
      正月初一
    </view>
</view>


