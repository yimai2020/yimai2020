.change{
  position: fixed;
  bottom: 30rpx;
  right: 30rpx;
  background-color: #fff;
  border-radius: 50rpx;
  width: 100rpx;
  height: 100rpx;
  z-index: 100;
  display: flex;
  justify-content: center;
  align-items: center;
  box-shadow: 0 6px 10px rgba(19, 19, 19, 0.678);
}

.title{
  width: 300rpx;
  border-radius: 10rpx;
  background: #cccc66;
  font-weight: 700;
  color: white;
  font-size: 35rpx;
  line-height: 45rpx;
  margin: 30rpx auto;
  display: flex;
  justify-content: center;
}


.content1{
  width: 650rpx;
  margin: 10rpx auto;
  background:#666699;
  padding: 20rpx;
  border-radius: 20rpx;
  color:white;
}
.swiper{
  width: 710rpx;
  border-radius: 10rpx;
  margin: 20rpx;
}