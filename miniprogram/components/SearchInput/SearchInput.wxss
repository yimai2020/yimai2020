.search_input {
  z-index: 10;
  position: fixed;
  width: 100%;
  background-color: #000;
  border-bottom-left-radius: 20rpx;
  border-bottom-right-radius: 20rpx;
}
.search_input navigator {
  height: 50rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #f6f6f6;
  border: 1px solid #ebe8e8;
  border-radius: 15rpx;
  color: #666;
  margin: 20rpx;
}
